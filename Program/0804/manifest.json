{
	"manifest_version": 2,
	"name": "The Senate",
	"version": "1.0",
	"description": "Spot fake news!",
	"permissions": ["activeTab", "declarativeContent", "storage"],
	"browser_action": {
		"default_popup": "web.html",
		"default_icon": {
			"16": "images/senate-16.png",
			"32": "images/senate-32.png",
			"64": "images/senate-64.png",
			"128": "images/senate-128.png"
		}
	},
	"icons": {
			"16": "images/senate-16.png",
			"32": "images/senate-32.png",
			"48": "images/senate-64.png",
			"128": "images/senate-128.png"
    }
}
